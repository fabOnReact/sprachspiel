<% @products.each do |product| %>
  <div class="col-md-3 col-sm-4 col-xs-4 product-partial" data-id="<%= product.id %>">
    <%= image_tag 'one', class:"icon numbers hidden", style:"", id: "icon-#{product.name}", data: {name: 'icon'} %>
    <%= image_tag product.picture, class: product.color, style: "height: 90px"%>     
  </div>
<% end %>